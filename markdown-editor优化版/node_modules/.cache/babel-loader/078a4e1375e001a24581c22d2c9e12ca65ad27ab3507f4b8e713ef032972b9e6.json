     class=\"menu-toggle\"\n          type=\"text\" \n          icon=\"ios-menu\"\n          @click=\"toggleFileMenu\"\n        />\n        <!-- 悬浮卡片式菜单 -->\n        <div v-if=\"showFileMenu\" class=\"file-menu-dropdown\" @click.stop>\n          <div class=\"menu-section\">\n            <h3 class=\"menu-title\">\n              <i-icon type=\"md-folder\" /> 文件操作\n            </h3>\n            <i-button long icon=\"md-save\" @click=\"saveFile\">保存</i-button>\n            <i-button long icon=\"md-folder-open\" @click=\"loadFile\">打开</i-button>\n            <i-button long icon=\"md-code\" @click=\"exportAsHTML\">导出HTML</i-button>\n            <i-button long icon=\"md-document\" @click=\"exportAsPDF\">导出PDF</i-button>\n          </div>\n\n          <div class=\"menu-section\">\n            <h3 class=\"menu-title\">\n              <i-icon type=\"md-color-palette\" /> 主题设置\n            </h3>\n            <i-radio-group :value=\"theme\" @on-change=\"changeTheme\">\n              <i-radio label=\"light\"><i-icon type=\"md-sunny\" /> 白天</i-radio>\n              <i-radio label=\"dark\"><i-icon type=\"md-moon\" /> 夜间</i-radio>\n            </i-radio-group>\n          </div>\n\n          <div class=\"menu-section\">\n            <h3 class=\"menu-title\">\n              <i-icon type=\"md-options\" /> 字体设置\n            </h3>\n            <i-select v-model=\"fontFamily\" style=\"width:100%;margin-bottom:10px\">\n              <i-option value=\"'Courier New', monospace\">等宽字体</i-option>\n              <i-option value=\"Arial, sans-serif\">Arial</i-option>\n              <i-option value=\"'Times New Roman', serif\">Times New Roman</i-option>\n            </i-select>\n            <i-select v-model=\"fontSize\" style=\"width:100%\">\n              <i-option value=\"14px\">小号</i-option>\n              <i-option value=\"16px\">中号</i-option>\n              <i-option value=\"18px\">大号</i-option>\n            </i-select>\n          </div>\n        </div>\n      </div>\n\n      <!-- 格式按钮 -->\n      <div class=\"format-buttons\">\n        <i-button \n          v-for=\"btn in toolbarButtons\" \n          :key=\"btn.name\"\n          @click=\"handleFormat(btn.action)\"\n          :disabled=\"btn.requiresSelection && !hasSelection\"\n          class=\"tool-btn\"\n        >\n          {{ btn.name }}\n        </i-button>\n      </div>\n\n      <!-- 主题切换按钮 -->\n      <i-button @click=\"changeTheme\" class=\"theme-btn\">\n        {{ theme === 'light' ? '🌙 夜间' : '☀️ 白天' }}\n      </i-button>\n    </div>\n\n    <!-- 编辑区和预览区 -->\n    <div class=\"editor-wrapper\">\n      <textarea\n        ref=\"editor\"\n        v-model=\"markdownContent\"\n        @input=\"updatePreview\"\n        @mouseup=\"updateSelection\"\n        class=\"editor\"\n        :style=\"editorStyle\"\n        placeholder=\"在这里输入Markdown内容...\"\n      ></textarea>\n      <div class=\"preview\" :style=\"editorStyle\" v-html=\"previewContent\"></div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { marked } from 'marked'\nimport hljs from 'highlight.js'\nimport { exportAsHTML, exportAsPDF } from './lib/exportUtils.js'\n\nexport default {\n  data() {\n    return {\n      markdownContent: '## 欢迎使用\\n\\n这是一个**Markdown编辑器**',\n      previewContent: '',\n      theme: 'light',\n      fontFamily: \"'Courier New', monospace\",\n      fontSize: \"14px\",\n      hasSelection: false,\n      showFileMenu: false,\n      toolbarButtons: [\n        { name: '加粗', action: 'bold', requiresSelection: true },\n        { name: '斜体', action: 'italic', requiresSelection: true },\n        { name: 'H1', action: 'h1', requiresSelection: true },\n        { name: 'H2', action: 'h2', requiresSelection: true },\n        { name: 'H3', action: 'h3', requiresSelection: true },\n        { name: '无序列表', action: 'ul', requiresSelection: true },\n        { name: '有序列表', action: 'ol', requiresSelection: true }\n      ]\n    }\n  },\n  computed: {\n    editorStyle() {\n      return {\n        fontFamily: this.fontFamily,\n        fontSize: this.fontSize\n      }\n    }\n  },\n  mounted() {\n    marked.setOptions({\n      highlight: (code) => hljs.highlightAuto(code).value\n    })\n    this.updatePreview()\n    document.addEventListener('click', this.closeFileMenu)\n  },\n  beforeDestroy() {\n    document.removeEventListener('click', this.closeFileMenu)\n  },\n  methods: {\n    toggleFileMenu() {\n      this.showFileMenu = !this.showFileMenu\n    },\n    closeFileMenu() {\n      this.showFileMenu = false\n    },\n    updateSelection() {\n      const editor = this.$refs.editor\n      this.hasSelection = editor.selectionStart !== editor.selectionEnd\n    },\n    updatePreview() {\n      this.previewContent = marked(this.markdownContent)\n    },\n    handleFormat(type) {\n      const editor = this.$refs.editor\n      const start = editor.selectionStart\n      const end = editor.selectionEnd\n      const text = this.markdownContent.slice(start, end)\n      \n      const formats = {\n        bold: { prefix: '**', suffix: '**' },\n        italic: { prefix: '_', suffix: '_' },\n        h1: { prefix: '# ', suffix: '' },\n        h2: { prefix: '## ', suffix: '' },\n        h3: { prefix: '### ', suffix: '' },\n        ul: { prefix: '- ', suffix: '' },\n        ol: { prefix: '1. ', suffix: '' }\n      }\n\n      if (formats[type]) {\n        const { prefix, suffix } = formats[type]\n        const newText = text.startsWith(prefix) ? \n          text.slice(prefix.length, suffix.length ? -suffix.length : undefined) : \n          prefix + text + suffix\n        \n        this.markdownContent = \n          this.markdownContent.slice(0, start) + \n          newText + \n          this.markdownContent.slice(end)\n        \n        this.$nextTick(() => {\n          editor.focus()\n          editor.setSelectionRange(start, start + newText.length)\n          this.updatePreview()\n        })\n      }\n    },\n    changeTheme(theme = null) {\n      this.theme = theme || (this.theme === 'light' ? 'dark' : 'light')